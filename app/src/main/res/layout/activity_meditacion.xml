<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Meditacion">



    <Button
        android:id="@+id/but_inicio3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/baseline_home_24"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="@+id/textView30"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView30"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:background="@drawable/morado"
        android:fontFamily="@font/corbel_bold"
        android:gravity="center"
        android:text="Meditacion"
        android:textColor="#FEE440"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="211dp"
        android:layout_height="209dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView30"
        app:layout_constraintVertical_bias="0.149"
        app:srcCompat="@drawable/meditacion1" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="324dp"
        android:layout_height="60dp"
        android:text="Escoge un opcion para ayudarte con tu meditación y relajar tu nivel de estrés y carga mental"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView30"
        app:layout_constraintVertical_bias="0.438" />

    <Button
        android:id="@+id/button6"
        android:layout_width="314dp"
        android:layout_height="100dp"
        android:background="@drawable/azul2"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.546"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.561" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="87dp"
        android:layout_height="87dp"
        android:layout_marginStart="10dp"
        android:elevation="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/button6"
        app:layout_constraintStart_toStartOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        app:srcCompat="@drawable/meditacion_guiada" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="180dp"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:elevation="3dp"
        android:text="Meditación Guiada:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/textView33"
        app:layout_constraintStart_toStartOf="@+id/textView33"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="187dp"
        android:layout_height="62dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="8dp"
        android:elevation="3dp"
        android:text="Revisa audios que puedan instruirte y ayudarte a realizar ejercicios de meditacion."
        android:textColor="#000000"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/textView32" />

    <Button
        android:id="@+id/button7"
        android:layout_width="314dp"
        android:layout_height="100dp"
        android:layout_marginTop="107dp"
        android:background="@drawable/amarillo"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button6"
        app:layout_constraintVertical_bias="0.011" />


    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="98dp"
        android:layout_height="93dp"
        android:elevation="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/button7"
        app:layout_constraintStart_toStartOf="@+id/button7"
        app:layout_constraintTop_toTopOf="@+id/button7"
        app:srcCompat="@drawable/temporizador" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:elevation="3dp"
        android:text="Temporizador:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/textView35"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView35"
        app:layout_constraintTop_toTopOf="@+id/button7" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="156dp"
        android:layout_height="57dp"
        android:layout_marginTop="5dp"
        android:elevation="3dp"
        android:text="Selecciona el tiempo que deseas pasar meditando. "
        android:textColor="#000000"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/button7"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <Button
        android:id="@+id/but_noti"
        android:layout_width="314dp"
        android:layout_height="100dp"
        android:background="@drawable/morado"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:layout_constraintVertical_bias="0.102" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="98dp"
        android:layout_height="93dp"
        android:elevation="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/but_noti"
        app:layout_constraintStart_toStartOf="@+id/but_noti"
        app:layout_constraintTop_toTopOf="@+id/but_noti"
        app:srcCompat="@drawable/notificacionn" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:elevation="3dp"
        android:text="Notificaciones:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/textView37"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView37"
        app:layout_constraintTop_toTopOf="@+id/but_noti" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="170dp"
        android:layout_height="51dp"
        android:elevation="3dp"
        android:text="Selecciona la fecha y hora en  la que deseas realizar la meditación."
        android:textColor="#000000"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/but_noti"
        app:layout_constraintEnd_toEndOf="@+id/but_noti"
        app:layout_constraintStart_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/but_noti"
        app:layout_constraintVertical_bias="0.693" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bot_login"
        android:text="Regresar"
        android:textColor="#000000"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView30"
        app:layout_constraintVertical_bias="0.992" />
</androidx.constraintlayout.widget.ConstraintLayout>